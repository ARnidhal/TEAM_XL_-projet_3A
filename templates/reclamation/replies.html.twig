{% extends 'form.html.twig' %}

{% block title %}Réponses pour la Réclamation {{ reclamation.id }}{% endblock %}

{% block body %}
  <h1>Réponses pour la Réclamation {{ reclamation.id }}</h1>
  <ul>
    {% for reply in reclamation.replies %}
      <li>{{ reply.content }}</li>
      
    {% endfor %}
  </ul>
{% endblock %}